{
  "name": "iventory",
  "productName": "iVentory",
  "version": "0.1.0",
  "description": "A Elm webapp over an Electron app :)",
  "main": "main.js",
  "scripts": {
    "mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds --ignore=release-builds --ignore=package-lock.json --ignore=assets",
    "win": "electron-packager . iVentory --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"iVentory\" --ignore=release-builds --ignore=package-lock.json --ignore=assets",
    "linux": "electron-packager . iVentory --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds --ignore=release-builds --ignore=package-lock.json --ignore=assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arieljimenez/iVentory.git"
  },
  "keywords": [
    "electron",
    "elm",
    "webapp",
    "spa"
  ],
  "author": {
    "name": "Ariel Jimenez",
    "email": "arieljimenez2025@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arieljimenez/iVentory/issues"
  },
  "homepage": "https://github.com/arieljimenez/iVentory#readme",
  "dependencies": {},
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-packager": "^11.2.0"
  }
}